{
  "name": "youtube-reload",
  "version": "2.4.0",
  "homepage": "https://gabrielogregorio.github.io/youtube-reload",
  "private": false,
  "main": "build/index.js",
  "engines": {
    "pnpm": ">=8.0.0",
    "node": ">=16.x"
  },
  "scripts": {
    "deploy": "gh-pages -d build",
    "dev": "vite --host",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --watch=false",
    "preview": "vite preview",
    "clear": "rm -rf build coverage",
    "check": "tsc -p .",
    "lint": "eslint -c ./.eslintrc.json --ext '.tsx,.ts' 'src/**' --fix",
    "prepare": "husky install",
    "format": "prettier src/* --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.22.5",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "autoprefixer": "^10.4.15",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-canonical-vocabulary": "^1.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-spellcheck": "^0.0.20",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.14.7",
    "vitest": "^0.25.8"
  }
}
